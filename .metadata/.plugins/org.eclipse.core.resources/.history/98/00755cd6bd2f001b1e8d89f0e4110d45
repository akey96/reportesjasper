	<queryString>
		<![CDATA[select h.date_h, h.date_h as fecha,  avg(t.temp) as promedio_temp, avg(h.humidity) as promedio_humedad, max(h.humidity) as max_humedad, min(h.humidity) as min_humedad, max(t.temp) as max_temperatura, min(t.temp) as min_temperatura from humidity as h
inner join temperature as t on h.date_h = t.date_h
where  
$X{BETWEEN, h.date_h ,  date_report_init,  date_report_end } 
GROUP BY fecha]]>
	</queryString>
