	<queryString>
		<![CDATA[SELECT ROW_NUMBER ( ) OVER (ORDER BY sales.id),
	sales.id,
	sales.user_id,
	sales.customer_id,
	sales.total_sale
FROM sales
	INNER JOIN customers ON 
	 customers.id = sales.customer_id 
	INNER JOIN users ON 
	 sales.user_id = users.id 
WHERE 
	 sales.user_id = 1 
	 AND $X{BETWEEN, sales.created_at,  date_report_init,  date_report_end }]]>
	</queryString>
